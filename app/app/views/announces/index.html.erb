<p id="notice"><%= notice %></p>

<h1>Announces</h1>

<table>
  <thead>
    <tr>
      <th>City</th>
      <th>Compagny</th>
      <th>Date</th>
      <th>Quizz</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @announces.each do |announce| %>
      <tr>
        <td><%= announce.city[:name] %></td>
        <td><%= announce.compagny[:name] %></td>
        <td><%= announce.date %></td>
        <td><%= announce.quiz[:name] %></td>
        <td><%= link_to 'Show', announce %></td>
        <td><%= link_to 'Edit', edit_announce_path(announce) %></td>
        <td><%= link_to 'Destroy', announce, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Announce', new_announce_path %>
